{
	"coc.preferences.extensionUpdateCheck": "daily",
	"coc.preferences.snippetStatusText": " Ⓢ ",
	"coc.source.buffer.enable": false,
	"coc.source.vimtex.enable": true,
	"suggest.noselect": true,
	"suggest.enablePreselect": false,
	"suggest.completionItemKindLabels": {
		"class": "\uf0e8",
		"color": "\ue22b",
		"constant": "\uf8fe",
		"default": "\uf29c",
		"enum": "\uf435",
		"enumMember": "\uf02b",
		"event": "\ufacd",
		"field": "\uf93d",
		"file": "\uf723",
		"folder": "\uf115",
		"function": "\u0192",
		"interface": "\uf417",
		"keyword": "\uf1de",
		"method": "\uf6a6",
		"module": "\uf40d",
		"operator": "\uf915",
		"property": "\ue624",
		"reference": "\ufa46",
		"snippet": "\ue60b",
		"struct": "\ufb44",
		"text": "\ue612",
		"typeParameter": "\uf728",
		"unit": "\uf475",
		"value": "\uf89f",
		"variable": "\ue71b"
	},
	"list.previousKeymap": "<C-k>",
	"list.nextKeymap": "<C-j>",
	"json.enable": true,
	"json.format.enable": true,
	"json.schemas": [],
	"json.trace.server": "verbose",
	"diagnostic.signLevel": "hint",
	"diagnostic.errorSign": "\uf467",
	"diagnostic.warningSign": "\uf071",
	"diagnostic.infoSign": "\uf129",
	"diagnostic.hintSign": "\uf864",
	"diagnostic.displayByAle": false,
	"diagnostic.refreshOnInsertMode": false,
	"diagnostic.checkCurrentLine": true,
	"diagnostic.virtualTextPrefix": " ❯❯❯ ",
	"diagnostic.virtualText": true,
	"markdownlint.onOpen": true,
	"markdownlint.onSave": true,
	"markdownlint.onChange": true,
	"markdownlint.config": {
		"default": true,
		"line-length": true
	},
	"snippets.ultisnips.enable": true,
	"snippets.snipmate.enable": false,
	"snippets.ultisnips.directories": [
		"$HOME/.config/nvim/UltiSnips",
		"$HOME/.config/nvim/plugged/vim-snippets/UltiSnips"
	],
	"snippets.extends": {
		"latex": [
			"tex",
			"texmath"
		]
	},
	"explorer.file.column.indent.indentLine": true,
	"explorer.file.showHiddenFiles": true,
	"explorer.icon.enableNerdfont": true,
	"explorer.keyMappings.global": {
		"u": "nodePrev",
		"e": "nodeNext",
		"<tab>": "actionMenu",
		"gl": "expandRecursive",
		"gh": "collapseRecursive",
		"i": [
			"wait",
			"expandable?",
			"expand",
			"open"
		],
		"<cr>": [
			"wait",
			"expandable?",
			"cd",
			"open"
		],
		"I": "open:vsplit",
		"o": [
			"wait",
			"expanded?",
			"collapse",
			"expand"
		],
		"O": "open:tab",
		"n": "collapse",
		"yp": "copyFilepath",
		"yn": "copyFilename",
		"yy": "copyFile",
		"dd": "cutFile",
		"pp": "pasteFile",
		"dD": "deleteForever",
		"a": "addFile",
		"M": "addDirectory",
		"cw": "rename",
		".": "toggleHidden",
		"zh": "toggleHidden",
		"R": "refresh",
		"?": "help",
		"q": "quit",
		"X": "systemExecute",
		"gd": "listDrive",
		"f": "search",
		"F": "searchRecursive",
		"B": "gotoSource:file",
		"b": "gotoSource:buffer",
		"[[": "sourcePrev",
		"]]": "sourceNext",
		"[d": "diagnosticPrev",
		"]d": "diagnosticNext",
		"[c": "gitPrev",
		"]c": "gitNext",
		"<<": "gitStage",
		">>": "gitUnstage"
	},
	"prettier.printWidth": 80,
	"prettier.disableLanguages": [],
	"prettier.formatterPriority": 1,
	"prettier.useTabs": true,
	"prettier.trailingComma": "all",
	"prettier.singleQuote": false,
	"translator.toLang": "zh_CN",
	"translator.enableHover": true,
	"translator.engines": [
		"youdao",
		"bing",
		"google",
		"haici"
	],
	"codeLens.enable": true,
	"coc.preferences.formatOnType": true,
	"coc.preferences.formatOnSaveFiletypes": [
		"json",
		"javascript",
		"typescript",
		"html",
		"css",
		"python",
		"vue",
		"svelte",
		"c",
		"c++",
		"go",
		"prisma",
		"markdown"
	],
	"python.linting.pylintEnabled": false,
	"python.formatting.provider": "yapf",
	"python.formatting.yapfArgs": [
		"--style",
		"{SPACES_AROUND_POWER_OPERATOR: True, SPACES_BEFORE_COMMENT: 1}"
	],
	"html.format.enable": true,
	"texlab.path": "$HOME/.config/coc/extensions/coc-texlab-data/texlab",
	"texlab.build.executable": "latexmk",
	"texlab.build.args": [
		"-xelatex",
		"-interaction=nonstopmode",
		"-synctex=1",
		"%f"
	],
	"texlab.build.onSave": true,
	"texlab.latexFormatter": "latexindent",
	"languageserver": {
		"golang": {
			"command": "gopls",
			"rootPatterns": ["go.mod"],
			"filetypes": ["go"]
		},
		"latex": {
			"command": "$HOME/.config/coc/extensions/coc-texlab-data/texlab",
			"filetypes": ["tex", "bib", "plaintex", "context"]
		}
	}
}
